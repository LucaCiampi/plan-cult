{
  "expo": {
    "name": "projet-fin-annee",
    "slug": "projet-fin-annee",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "acme",
    "icon": "./assets/images/icon.png",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "userInterfaceStyle": "automatic"
  }
}
